	Comments -->
<deploymentConfiguration>
	<logRules maxBytes="32Mb" level="verbose" />
	<localNode host="rhlpa010.rccl.com" />
	<replicationFarmSet>
		<replicationFarm name="ts_server">
			<nodeRef useNode="$tsserver^" />
		</replicationFarm>
	</replicationFarmSet>

	<definition name="DCRs">
		<source>
			<iwStore>
				<remoteDiff
					area="/default/main/$brand^[ROYAL]/$domain^[royalcaribbean.com]/WORKAREA/content">
					<pathSpecification>
						<path name="/templatedata" />
						<filters>
							<includePattern regex="\.xml$" />
						</filters>
					</pathSpecification>
				</remoteDiff>
			</iwStore>

		</source>
		<target>
			<targetTeamsite
				area="/default/main/$brand^[ROYAL]/$domain^[royalcaribbean.com]/WORKAREA/content/templatedata"
				applyExtAttrs="yes" />
			<transferRules doDeletes="yes" checksumVerify="yes" />
			<comparisonRules dateDifferent="yes" />
			<permissionRules file="0755" directory="0755" user="cont_adm"
				group="cont_adm" />
			<replicationFarmLink>
				<internal name="ts_server" />
			</replicationFarmLink>
		</target>
	</definition>

	<definition name="Assets">
		<source>
			<iwStore>
				<remoteDiff
					area="/default/main/$brand^[ROYAL]/$domain^[royalcaribbean.com]/WORKAREA/content">
					<pathSpecification>
						<path name="media" />
					</pathSpecification>
				</remoteDiff>
			</iwStore>
		</source>
		<target>
			<targetTeamsite
				area="/default/main/$brand^[ROYAL]/$domain^[royalcaribbean.com]/WORKAREA/content/media" />
			<transferRules doDeletes="yes" checksumVerify="no" />
			<comparisonRules dateDifferent="yes" />
			<permissionRules file="0755" directory="0755" user="cont_adm"
				group="cont_adm" />
			<replicationFarmLink>
				<internal name="ts_server" />
			</replicationFarmLink>
		</target>
	</definition>


	<deployment transactional="no">
		<execDeploymentTask useDefinition="DCRs" />
		<!-- <execDeploymentTask useDefinition="Assets"/> -->
	</deployment>
</deploymentConfiguration>
